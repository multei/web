{
  "name": "@multei/web",
  "version": "0.1.0",
  "description": "Multei! website project",
  "repository": {
    "type": "git",
    "url": "https://github.com/multei/web"
  },
  "bugs": {
    "url": "https://github.com/multei/web/issues"
  },
  "keywords": [
    "gatsby",
    "multei",
    "react",
    "web"
  ],
  "author": "Multei!",
  "private": true,
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "clean:install": "rm -rf node_modules/sharp && npm install",
    "ci": "npm run testc && npm run codecov",
    "codecov": "codecov",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "dev": "npm run develop",
    "develop": "gatsby develop --host ${npm_package_config_localhost} --port ${npm_package_config_port}",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "test": "jest",
    "testc": "jest --coverage",
    "testu": "jest -u",
    "testw": "jest --watch",
    "test:e2e": "npm run develop & wait-on http://${npm_package_config_localhost}:${npm_package_config_port} && npm run cy:open",
    "test:e2e:ci": "npm run develop & wait-on http://${npm_package_config_localhost}:${npm_package_config_port} && npm run cy:run"
  },
  "config": {
    "localhost": "localhost",
    "port": 8000

  },
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "4.9.9",
    "@material-ui/icons": "4.9.1",
    "@material-ui/lab": "4.0.0-alpha.48",
    "@material-ui/styles": "4.9.6",
    "clsx": "1.1.0",
    "gatsby": "2.20.15",
    "gatsby-image": "2.3.2",
    "gatsby-plugin-canonical-urls": "2.2.1",
    "gatsby-plugin-layout": "1.2.1",
    "gatsby-plugin-manifest": "2.3.3",
    "gatsby-plugin-material-ui": "2.1.6",
    "gatsby-plugin-offline": "3.1.2",
    "gatsby-plugin-prefetch-google-fonts": "1.4.3",
    "gatsby-plugin-react-helmet": "3.2.1",
    "gatsby-plugin-sharp": "2.5.4",
    "gatsby-source-filesystem": "2.2.2",
    "gatsby-transformer-sharp": "2.4.4",
    "muy": "0.15.4",
    "prop-types": "15.7.2",
    "query-string": "6.12.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.0.0",
    "use-global-hook": "0.1.12"
  },
  "devDependencies": {
    "@hot-loader/react-dom": "16.13.0",
    "@testing-library/cypress": "6.0.0",
    "axe-core": "3.5.3",
    "axios": "0.19.2",
    "babel-jest": "25.3.0",
    "babel-preset-gatsby": "0.3.1",
    "codecov": "3.6.5",
    "cypress": "4.3.0",
    "cypress-axe": "0.8.1",
    "cypress-file-upload": "4.0.4",
    "husky": "4.2.5",
    "identity-obj-proxy": "3.0.0",
    "jest": "25.3.0",
    "netlify-cli": "2.41.0",
    "prettier": "2.0.4",
    "react-test-renderer": "16.13.1",
    "wait-on": "4.0.2"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test",
      "pre-push": "npm test && npm run test:e2e:ci",
      "post-update": "npm install"
    }
  }
}
