{
  "name": "@multei/web",
  "version": "0.1.0",
  "description": "Multei! website project",
  "repository": {
    "type": "git",
    "url": "https://github.com/multei/web"
  },
  "bugs": {
    "url": "https://github.com/multei/web/issues"
  },
  "keywords": [
    "gatsby",
    "multei",
    "react",
    "web"
  ],
  "author": "Multei!",
  "private": true,
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "clean:install": "rm -rf node_modules/sharp && npm install",
    "ci": "npm run testc && npm run codecov",
    "codecov": "codecov",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "deploy": "git push",
    "dev": "npm run develop",
    "develop": "gatsby develop --host ${npm_package_config_localhost} --port ${npm_package_config_port}",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "open:local": "open http://${npm_package_config_localhost}:${npm_package_config_port}",
    "ports": "lsof -iTCP -sTCP:LISTEN -n -P",
    "precommit": "sh ./scripts/test-staged.sh",
    "prepush": "npm test && npm run test:e2e:ci",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "test": "jest",
    "testc": "jest --coverage",
    "testu": "jest -u",
    "testw": "jest --watch",
    "test:e2e": "npm run develop & wait-on http://${npm_package_config_localhost}:${npm_package_config_port} && npm run cy:open",
    "test:e2e:ci": "npm run develop & wait-on http://${npm_package_config_localhost}:${npm_package_config_port} && npm run cy:run"
  },
  "config": {
    "localhost": "localhost",
    "port": 8000
  },
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "4.9.13",
    "@material-ui/icons": "4.9.1",
    "@material-ui/lab": "4.0.0-alpha.52",
    "@material-ui/styles": "4.9.13",
    "clsx": "1.1.0",
    "gatsby": "2.21.21",
    "gatsby-image": "2.4.3",
    "gatsby-plugin-canonical-urls": "2.3.1",
    "gatsby-plugin-layout": "1.3.1",
    "gatsby-plugin-manifest": "2.4.2",
    "gatsby-plugin-material-ui": "2.1.8",
    "gatsby-plugin-offline": "3.2.1",
    "gatsby-plugin-prefetch-google-fonts": "1.4.3",
    "gatsby-plugin-react-helmet": "3.3.1",
    "gatsby-plugin-sharp": "2.6.2",
    "gatsby-plugin-sitemap": "2.4.2",
    "gatsby-source-filesystem": "2.3.1",
    "gatsby-transformer-sharp": "2.5.2",
    "metax": "1.0.2",
    "muy": "1.0.2",
    "prop-types": "15.7.2",
    "query-string": "6.12.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.0.0",
    "use-global-hook": "0.1.12"
  },
  "devDependencies": {
    "@hot-loader/react-dom": "16.13.0",
    "@stryker-mutator/core": "^3.1.0",
    "@stryker-mutator/javascript-mutator": "^3.1.0",
    "@stryker-mutator/jest-runner": "^3.1.0",
    "@testing-library/cypress": "6.0.0",
    "@testing-library/jest-dom": "^5.7.0",
    "@testing-library/react": "^10.0.4",
    "axe-core": "3.5.3",
    "axios": "0.19.2",
    "babel-jest": "25.5.1",
    "babel-preset-gatsby": "0.4.1",
    "codecov": "3.6.5",
    "cypress": "4.5.0",
    "cypress-axe": "0.8.1",
    "cypress-file-upload": "4.0.6",
    "dotenv": "8.2.0",
    "husky": "4.2.5",
    "identity-obj-proxy": "3.0.0",
    "jest": "25.5.4",
    "netlify-cli": "2.51.0",
    "prettier": "2.0.5",
    "react-test-renderer": "16.13.1",
    "wait-on": "5.0.0"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit",
      "pre-push": "npm run prepush",
      "post-update": "npm install"
    }
  }
}
