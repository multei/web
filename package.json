{
  "name": "@multei/web",
  "version": "0.1.0",
  "description": "Multei! website project",
  "repository": {
    "type": "git",
    "url": "https://github.com/multei/web"
  },
  "bugs": {
    "url": "https://github.com/multei/web/issues"
  },
  "keywords": [
    "gatsby"
  ],
  "author": "Multei!",
  "private": true,
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "clean:install": "rm -rf node_modules/sharp && npm install",
    "ci": "npm run testc && npm run codecov",
    "codecov": "codecov",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "dev": "npm run develop",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "test": "jest",
    "testc": "jest --coverage",
    "testu": "jest -u",
    "testw": "jest --watch",
    "test:e2e": "start-server-and-test develop http://localhost:8000 cy:open",
    "test:e2e:ci": "start-server-and-test develop http://localhost:8000 cy:run"
  },
  "contributors": [
    {
      "name": "Beto Maia",
      "email": "beto@multei.com.br",
      "url": "https://github.com/adalbertomaia"
    },
    {
      "name": "Jimmy Andrade",
      "email": "jimmy@multei.com.br",
      "url": "https://github.com/jimmyandrade"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "4.9.7",
    "@material-ui/icons": "4.9.1",
    "@material-ui/lab": "4.0.0-alpha.46",
    "@material-ui/styles": "4.9.6",
    "clsx": "1.1.0",
    "gatsby": "2.20.4",
    "gatsby-image": "2.3.1",
    "gatsby-plugin-canonical-urls": "2.2.1",
    "gatsby-plugin-layout": "1.2.1",
    "gatsby-plugin-manifest": "2.3.2",
    "gatsby-plugin-material-ui": "2.1.6",
    "gatsby-plugin-offline": "3.1.1",
    "gatsby-plugin-prefetch-google-fonts": "1.4.3",
    "gatsby-plugin-react-helmet": "3.2.1",
    "gatsby-plugin-sharp": "2.5.3",
    "gatsby-source-filesystem": "2.2.2",
    "gatsby-transformer-sharp": "2.4.2",
    "muy": "0.7.2",
    "prop-types": "15.7.2",
    "query-string": "6.11.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.0.0-beta.2",
    "use-global-hook": "0.1.12"
  },
  "devDependencies": {
    "@hot-loader/react-dom": "16.13.0",
    "@testing-library/cypress": "6.0.0",
    "axe-core": "3.5.2",
    "axios": "0.19.2",
    "babel-jest": "25.1.0",
    "babel-preset-gatsby": "0.3.1",
    "codecov": "3.6.5",
    "cypress": "4.2.0",
    "cypress-axe": "0.5.3",
    "cypress-file-upload": "4.0.4",
    "husky": "4.2.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "25.1.0",
    "netlify-cli": "2.40.0",
    "prettier": "2.0.2",
    "react-test-renderer": "16.13.1",
    "start-server-and-test": "1.10.11"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test",
      "pre-push": "npm test && npm run test:e2e:ci",
      "post-update": "npm install"
    }
  }
}
