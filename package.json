{
  "name": "@multei/web",
  "version": "0.1.0",
  "description": "Multei! website project",
  "repository": {
    "type": "git",
    "url": "https://github.com/multei/web"
  },
  "bugs": {
    "url": "https://github.com/multei/web/issues"
  },
  "keywords": [
    "gatsby",
    "multei",
    "react",
    "web"
  ],
  "author": "Multei!",
  "private": true,
  "scripts": {
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "build:ci": "CI=true gatsby build",
    "build:local": "gatsby build",
    "ci": "npm run build:ci && npm run testc && npm run codecov",
    "clean": "gatsby clean",
    "clean:install": "rm -rf node_modules/sharp && npm install",
    "codecov": "codecov",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "dev": "npm run develop",
    "develop": "gatsby develop --host ${npm_package_config_localhost} --port ${npm_package_config_port}",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "format:staged": "sh ./scripts/format-staged.sh",
    "hook:precommit": "npm run format:staged && sh ./scripts/test-staged.sh",
    "hook:prepush": "npm run format && npm test && npm run test:e2e:prepush",
    "open:local": "open http://${npm_package_config_localhost}:${npm_package_config_port}",
    "ports": "lsof -iTCP -sTCP:LISTEN -n -P",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "test": "jest",
    "test:e2e:ci": "npm run develop & wait-on http://${npm_package_config_localhost}:${npm_package_config_port} && npm run cy:run",
    "test:e2e:prepush": "npm run develop & npm run cy:run",
    "testc": "jest --coverage",
    "testu": "jest -u",
    "testw": "jest --watch"
  },
  "config": {
    "localhost": "localhost",
    "port": 8001
  },
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "4.9.14",
    "@material-ui/icons": "4.9.1",
    "@material-ui/lab": "4.0.0-alpha.53",
    "@material-ui/styles": "4.9.14",
    "clsx": "1.1.0",
    "gatsby": "2.22.9",
    "gatsby-image": "2.4.5",
    "gatsby-plugin-canonical-urls": "2.3.2",
    "gatsby-plugin-layout": "1.3.2",
    "gatsby-plugin-manifest": "2.4.9",
    "gatsby-plugin-material-ui": "2.1.8",
    "gatsby-plugin-offline": "3.2.7",
    "gatsby-plugin-prefetch-google-fonts": "1.4.3",
    "gatsby-plugin-react-helmet": "3.3.2",
    "gatsby-plugin-sharp": "2.6.9",
    "gatsby-plugin-sitemap": "2.4.3",
    "gatsby-source-filesystem": "2.3.8",
    "gatsby-transformer-sharp": "2.5.3",
    "metax": "1.1.0",
    "muy": "1.0.7",
    "prop-types": "15.7.2",
    "query-string": "6.12.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.0.0",
    "recoil": "0.0.7",
    "use-global-hook": "0.1.12"
  },
  "devDependencies": {
    "@hot-loader/react-dom": "16.13.0",
    "@stryker-mutator/core": "3.2.4",
    "@stryker-mutator/javascript-mutator": "3.2.4",
    "@stryker-mutator/jest-runner": "3.2.4",
    "@testing-library/cypress": "6.0.0",
    "@testing-library/jest-dom": "5.9.0",
    "@testing-library/react": "10.0.4",
    "axe-core": "3.5.3",
    "axios": "0.19.2",
    "babel-jest": "26.0.1",
    "babel-preset-gatsby": "0.4.7",
    "codecov": "3.7.0",
    "cypress": "4.6.0",
    "cypress-axe": "0.8.1",
    "cypress-file-upload": "4.0.7",
    "dotenv": "8.2.0",
    "husky": "4.2.5",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.0.1",
    "netlify-cli": "2.51.0",
    "prettier": "2.0.5",
    "react-test-renderer": "16.13.1",
    "wait-on": "5.0.0"
  },
  "engines": {
    "node": ">=12.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run hook:precommit",
      "pre-push": "npm run hook:prepush"
    }
  },
  "resolutions": {
    "http-proxy": ">=1.18.1"
  }
}
